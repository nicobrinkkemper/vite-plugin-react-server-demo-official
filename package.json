{
  "name": "vite-plugin-react-server-demo-official",
  "repository": "https://github.com/nicobrinkkemper/vite-plugin-react-server-demo-official",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "demo": "rm todos.db && PUBLIC_ORIGIN='http://localhost:3000' BASE_URL='/' npm run build && NODE_OPTIONS='--conditions react-server' node dist/server/server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run env:dev-server -- vite",
    "start": "npm run env:start -- vite",
    "preview": "npm run build:preview && npm run preview:start",
    "env:gh": "PUBLIC_ORIGIN='https://nicobrinkkemper.github.io' BASE_URL='/vite-plugin-react-server-demo-official/'",
    "env:preview": "PUBLIC_ORIGIN='http://localhost:4173' BASE_URL='/vite-plugin-react-server-demo-official/'",
    "env:dev": "NODE_ENV=development BASE_URL='/' PUBLIC_ORIGIN='http://localhost:5173'",
    "env:dev-server": "NODE_OPTIONS='--conditions react-server' && npm run env:dev",
    "env:start": "NODE_ENV=development BASE_URL='/' PUBLIC_ORIGIN='http://localhost:5173'",
    "build": "vite build --app",
    "build:gh": "npm run env:gh -- vite build --app",
    "build:preview": "npm run env:preview -- vite build --app",
    "preview:start": "npm run env:preview -- vite preview",
    "debug-build": "npm run env:dev -- vite build --app --mode development",
    "postinstall": "patch-package",
    "clean-install": "npm install react@experimental react-dom@experimental react-server-dom-esm",
    "patch-oss": "patch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "acorn-loose": "^8.4.0",
    "happy-dom": "^17.1.8",
    "patch-package": "^8.0.0",
    "react": "^0.0.0-experimental-c0464aed-20250523",
    "react-dom": "^0.0.0-experimental-c0464aed-20250523",
    "react-server-dom-esm": "^0.0.1",
    "tsx": "^4.19.2",
    "vite": "^6.3.2",
    "webpack-sources": "^3.3.0"
  },
  "dependencies": {
    "express": "^5.1.0",
    "vite-plugin-react-server": "^1.1.20"
  }
}
