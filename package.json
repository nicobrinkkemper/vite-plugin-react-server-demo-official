{
  "name": "vite-plugin-react-server-demo-official",
  "repository": "https://github.com/nicobrinkkemper/vite-plugin-react-server-demo-official",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:static && npm run build:client && npm run build:server",
    "dev": "NODE_OPTIONS='--conditions=react-server' vite",
    "preview": "npm run build && vite preview",
    "start": "vite",
    "build:static": "vite build",
    "build:client": "vite build --ssr",
    "build:server": "NODE_OPTIONS='--conditions=react-server' vite build --ssr",
    "debug-build": "NODE_ENV=development npm run build:static -- --mode development && NODE_ENV=development npm run build:client -- --mode development && NODE_ENV=development npm run build:server -- --mode development",
    "preview:static": "NODE_OPTIONS='--conditions=react-server' vite preview",
    "postinstall": "patch-package",
    "clean-install": "npm install react@experimental react-dom@experimental react-server-dom-esm",
    "patch-oss": "patch"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "acorn-loose": "^8.4.0",
    "happy-dom": "^17.1.8",
    "patch-package": "^8.0.0",
    "react": "^0.0.0-experimental-38ef6550-20250508",
    "react-dom": "^0.0.0-experimental-38ef6550-20250508",
    "react-server-dom-esm": "^0.0.1",
    "tsx": "^4.19.2",
    "vite": "^6.3.2",
    "vite-plugin-react-server": "^1.1.6"
  }
}
